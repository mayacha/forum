RewriteEngine On
Options +FollowSymLinks
#ligne suivante Ã  retirer pour empecher l acces aux repertoires
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-l
RewriteRule ^([a-zA-Z0-9_:\-]+)/?$ index.php?page=$1 [L]
RewriteRule ^category/([a-zA-Z0-9_:\-\.\?\!]+)/?$ index.php?page=category&category=$1 [L]
RewriteRule ^category/([a-zA-Z0-9_:\-\.\?\!]+)/?$ index.php?page=category&topic=$1 [L]
RewriteRule ^profil/([a-zA-Z0-9_:\-\.\?\!]+)/?$ index.php?page=profil&login=$1 [L]
RewriteRule ^admin/([a-zA-Z0-9_:\-\.\?\!]+)/?$ index.php?page=admin&page_admin=$1 [L]

